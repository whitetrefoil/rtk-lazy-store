{
  "name": "@whitetrefoil/rtk-lazy-store",
  "version": "1.0.0",
  "description": "A lazy store impl. for Redux w/ Redux Toolkit",
  "keywords": [
    "react",
    "redux",
    "redux-toolkit",
    "lazy",
    "store"
  ],
  "files": [
    "lib",
    "src",
    "tests",
    "tsconfig*.json"
  ],
  "type": "module",
  "source": "src/main.ts",
  "main": "lib/main.js",
  "exports": {
    ".": "./lib/main.js"
  },
  "scripts": {
    "prepack": "yarn build; node scripts/prepack.cjs",
    "postpack": "node scripts/postpack.cjs",
    "build": "tsc -p src/tsconfig.json --noEmit false",
    "test": "rimraf test_results && eslint . ; jest --no-cache",
    "test-ci": "rimraf test_results && eslint -o test_results/eslint/junit.xml -f junit . ; jest -i --no-cache",
    "watch": "yarn build -w"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:whitetrefoil/rtk-lazy-store.git"
  },
  "author": "WhiteTrefoil <whitetrefoil@gmail.com>",
  "license": "Unlicense",
  "engines": {
    "node": ">=14.18.0"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.25",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@whitetrefoil/eslint-config": "^0.31.0",
    "eslint": "^8.27.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-junit": "^14.0.1",
    "jest-localstorage-mock": "^2.4.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "react": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0"
  },
  "packageManager": "yarn@3.3.0"
}
